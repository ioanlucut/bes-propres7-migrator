{
  "name": "migrator_propres7-node",
  "version": "1.0.0",
  "description": "A migrator from .txt song lyrics to PP7 pro format.",
  "main": "index.ts",
  "scripts": {
    "test:ci": "NODE_ENV=test jest --runInBand --no-cache",
    "test:watch": "NODE_ENV=test TZ='Europe/Berlin' jest --watch --logHeapUsage",
    "test": "is-ci-cli test:ci test:watch",
    "migrate:local": "ts-node runner.ts",
    "migrate:remote": "dotenv -e .env.remote ts-node runner.ts"
  },
  "author": "Ioan Lucut",
  "dependencies": {
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "fs-extra": "^11.1.1",
    "google-protobuf": "^3.21.2",
    "lodash": "^4.17.21",
    "protobufjs": "^7.2.3",
    "recursive-readdir": "^2.2.3",
    "ts-proto": "^1.147.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.0.0",
    "@types/recursive-readdir": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.6.1",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
}
